config_version=5

[application]

config/name="UntitledShotgunProject"
config/tags=PackedStringArray("v4.3")
run/main_scene="res://screen/menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
config/icon="res://asset/sprite/icon.svg"

[autoload]

